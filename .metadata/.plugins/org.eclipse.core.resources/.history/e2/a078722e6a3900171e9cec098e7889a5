import java.awt.Color;

public class FaceRect {
	public ColorRect[][] face;

	public FaceRect(Color color, int x, int y, int SIZE_MOD) {
		for (int r = 0; r < face.length; r++) {
			for (int c = 0; c < face[0].length; c++) {
				face[r][c] = new ColorRect(x + c * SIZE_MOD, y + r * SIZE_MOD, SIZE_MOD, color);
			}
		}
	}
}
