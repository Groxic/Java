import java.awt.Color;

public class Edge extends Piece {
	private Color f1; // f1 faces orientation
	private Color f2;

	public Edge(char orientation, Color f1, Color f2) {
		super(orientation);
		this.f1 = f1;
		this.f2 = f2;
	}

	@Override
	public Color getF1() {
		return f1;
	}

	@Override
	public Color getF2() {
		return f2;
	}

	@Override
	public Color getF3() {
		return null;
	}
}
