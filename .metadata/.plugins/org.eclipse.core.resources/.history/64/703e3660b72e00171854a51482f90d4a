
public class Piece {
	private char orientation;
	private int x;
	private int y;
	private int z;

	public Piece(int x, int y, int z, char orient) {
		this.orientation = orient;
		this.x = x;
		this.y = y;
		this.z = z;
	}

	public int getOrient() {
		return orientation;
	}
	
	public void setOrient(char orientation) {
		this.orientation = orientation;
	}
	
	public int getX() {
		return x;
	}

	public int getY() {
		return y;
	}

	public int getZ() {
		return z;
	}
	
	public void setX(int x) {
		this.x = x;
	}
	
	public void setY(int y) {
		this.y = y;
	}
	
	public void setZ(int z) {
		this.z = z;
	}
}
