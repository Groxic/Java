import java.awt.Color;

public class Center extends Piece{
	private Color f1;
	
	public Edge(int x, int y, int z, char orientation, Color f1, Color f2) {
		super(x, y, z, orientation);
		this.f1 = f1;
	}

	public Color getF1() {
		return f1;
	}

	public Color getF2() {
		return f2;
	}
	
	public Color getF3() {
		return null;
	}

}
